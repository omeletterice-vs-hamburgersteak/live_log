<h1>新しいライブを登録</h1>

<%= form_with(model: @live_event, local: true) do |form| %>
  <div>
    <%= form.label :title, "ライブ名" %>
    <%= form.text_field :title %>
  </div>

  <div>
    <%= form.label :date, "日付" %>
    <%= form.date_select :date %>
  </div>

  <div>
    <%= form.label :venue, "会場" %>
    <%= form.text_field :venue %>
  </div>

  <div>
    <%= form.label :artist, "出演者" %>
    <%= form.text_field :artist %>
  </div>

  <div>
    <%= form.submit "登録" %>
  </div>
<% end %>
